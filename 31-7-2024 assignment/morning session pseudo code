1.To print the one is adult based his/her age 
   Note: Dont print if not adult
    function IsAdult(age) 
        isAdult = (age >= 18);
        return isAdult; 

    function TestIsAdult()
        Input age 
        if(IsAdult(age)) 
            print "You are an adult."
        end if


2. Take a num 10, if num greater than 5
   print "num as 'greater than 5"

function IsGreaterThan5(num)
   isGreater = (num > 5)
   return isGreater

function TestIsGreaterThan5()
  Input num
  if(IsGreaterThan5(num))
       print "Greater than 5"
  end if


3.To print 'number is positive' if num > 0
       it should not print if not positive 

function IsPositive(num)
   isPositive = (num > 0)
   return isPositive

function TestIsPositive()
  Input num
  if(IsPositive(num))
       print "Positive number"
  end if


4.if temp > 25, print it is hot outside.

function IsTempHot(temp)
   isTempHot = (temp > 25)
   return isTempHot

function TestIsTempHot()
  Input temp
  if(IsTempHot(temp))
       print "It is hot outside"
  end if


5.To print the is adult or not based his/her age. 

function IsAdultNotAdult(age) 
        isAdult = (age >= 18);
        return isAdult; 

function TestIsAdultNotAdult()
        Input age 
        if(IsAdult(age)) 
            print 'You are an adult.'
        else 
            print 'You are not an adult.'
        end if


6.Input mark, if IsMarkGreaterThan40(mark) 
    print pass else fail 

function IsMarkGreaterThan40(mark)
    isMarkGreaterThan40 = (mark > 40)
    return isMarkGreaterThan40

function TestIsMarkGreaterThan40()
    Input mark 
    if (IsMarkGreaterThan40(mark))
        print "pass" 
    else 
        print "fail" 
    end if


7.Input number, check it is positive or negative 

function IsPositiveOrNegative(num)
   isPositiveOrNegative = (num > 0)
   return isPositiveOrNegative

function TestIsPositiveOrNegative()
  Input num
  if (IsPositiveOrNegative(num))
       print "Positive number"
  else
       print"Negative number"
  end if


8.input doctorSalary/month, 
    check if is 6 digit salary or not 

function IsSalary6(salary)
   isSalary6 = (salary >= 100000)
   return isSalary6

function IsSalary6()
  Input salary
  if (IsSalary6(salary))
       print "Salary is 6 digit"
  else
       print"Salary is not 6 digit"
  end if


9.check tumour 
                is cancerous if  malignant 
                or not cancerous if benign
            Note: tumour is string of text either malignant or benign

function IsCancerous(tumour)
    isCancerous = (tumour == "malignant")
    return isCancerous

function TestIsCancerous()
    Input tumour
    if(IsCancerous(tumour))
        print "Cancerous"
    else
        print "Not cancerous"
    end if


10. To check the person adult, teenager or child 
    for given age 
    function IsAdult(age) 
        isAdult = (age >= 18);
        return isAdult; 

    function IsTeenager(age) 
        isTeenager = (age >= 13);
        return isTeenager; 

    function TestIsAdultTeenagerOrChild()
        Input age 
        if(IsAdult(age)) 
            print 'You are an adult.'
        else if(IsTeenager(age)) 
            print 'You are a teenager.'
        else
            print 'You are a child.'
        end if


11.check for car maintenance
    cases :
        'O' = Oil Change, "Change Oil and Replace Oil Filter"
        'T' = Tire Rotaion, "Rotate Tire and align them properly"
        'B' = Brake, "Inspect the brake and replace them if necessary" 
        'A' = Battery,         "Battery Check and Replace them"
        Default: "Some other reason"

function SuggestMaintenance(serviceOption)
        string suggestion;
        switch serviceOption
            case 'O':
                suggestion = "Change Oil and Replace Oil Filter"
                break
            case 'T':
                suggestion = "Rotate Tire and align them properly"
                break
            case 'B':
                suggestion = "Inspect the brake and replace them if necessary"
                break
            case 'A':
                suggestion = "Battery Check and Replace them"
                break
            default: 
                suggestion = "Some other reason"
                break 
        end switch 
        return suggestion
    function TestSuggestMaintenance() 
        Input serviceOption 
        suggestion = SuggestMaintenance(serviceOption)
        print suggestion    
