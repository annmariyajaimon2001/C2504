static bool TryDiv(int x, int y, out int res)
{
    try
    {
        res = x / y;
        return true;
    }
    catch
    {
        res = 0;
        return false;
    }
}
static void Main(string[] args)
{
    int a = 10;
    int b = 2;
    //int result = 0;
    if (TryDiv(a, b, out int result))
    {

        Console.WriteLine(result);
    }
    else
    {
        Console.WriteLine("division failed");
    }
}
