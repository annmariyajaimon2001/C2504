7

pseudo:

function ReadAndFindMinSalaryOfNDoctors():
    MinSalary = -1
    Input N  
    for i = 1 .. N 
        Input salary
        if i == 1 then 
             MinSalary = salary 
             continue 
        end if 

        if salary < MinSalary then 
            MinSalary = salary
        end if 

c#:
class ReadAndFindMinSalaryOfNDoctors
{
static int ReadAndFindMinSalaryOfNDoctors()
    {
        int minSalary = -1;

        Console.WriteLine("Enter the number of doctors: ");
        int N = int.Parse(Console.ReadLine());

        for (int i = 1; i <= N; i++)
        {
            Console.WriteLine($"Enter the salary for doctor {i}: ");
            int salary = int.Parse(Console.ReadLine());

            if (i == 1)
            {
                minSalary = salary;
                continue;
            }

            if (salary < minSalary)
            {
                minSalary = salary;
            }
        }

        return minSalary;
    }
 static void Main(string[] args)
    {
        int minSalary = ReadAndFindMinSalaryOfNDoctors();
        Console.WriteLine("Minimum salary among the doctors: " + minSalary);
    }
}

