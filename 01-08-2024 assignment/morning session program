function ReadAndFindSumOfDoctorsSalariesTillZero()
    sum = 0 
    do 
        input salary 
        if !(salary > 0 || salary == 0)
            print "Input valid salary"
            continue 
        end if 
        
        if salary != 0 
            sum += salary
        end if  
    while salary != 0
    print sum 

